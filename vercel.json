{
    "functions": {
        "api/index.py": {
            "maxDuration": 30
        },
        "api/extract.py": {
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/csv-upload",
            "destination": "/api/index.py"
        },
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.py"
        }
    ]
}