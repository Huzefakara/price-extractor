{
    "version": 2,
    "builds": [
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/extract",
            "dest": "/api/extract.py"
        },
        {
            "src": "/api/compare-csv",
            "dest": "/api/compare-csv.py"
        },
        {
            "src": "/csv-upload",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ]
}
